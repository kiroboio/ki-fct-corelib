{
  "name": "ki-fct-corelib",
  "version": "1.0.4",
  "description": "Kirobo.io FCT Core library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register \"src/**/*.test.ts\" --timeout 100000",
    "test:fct": "mocha -r ts-node/register src/batchMultiSigCall/test/*.test.ts",
    "test:utils": "mocha -r ts-node/register src/utils/*.test.ts",
    "build": "rm -rf dist/ & tsc --project tsconfig.build.json",
    "fct": "ts-node scripts/fct.script.ts",
    "fct:struct": "ts-node scripts/fct_struct.script.ts",
    "fct:get": "ts-node scripts/fct_get.script.ts",
    "fct:approvals": "ts-node scripts/fct_approvals.script.ts",
    "fct:estimate": "ts-node scripts/estimate.script.ts",
    "fct:getDomainData": "ts-node scripts/getDomainData.ts",
    "lint": "eslint  --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiroboio/ki-fct-corelib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kiroboio/ki-fct-corelib/issues"
  },
  "release": {
    "branches": [
      "master",
      "develop"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  },
  "homepage": "https://github.com/kiroboio/ki-fct-corelib#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@types/chai": "^4.3.4",
    "@types/graphlib": "^2.1.8",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hardhat": "^2.12.2",
    "hardhat-dependency-compiler": "^1.1.3",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^20.1.0",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@kirobo/ki-eth-fct-provider-ts": "https://github.com/kiroboio/ki-eth-fct-provider-ts.git#develop",
    "@metamask/eth-sig-util": "^5.0.2",
    "bignumber.js": "^9.1.0",
    "ethers": "^5.7.2",
    "graphlib": "^2.1.8",
    "lodash": "^4.17.21"
  }
}
