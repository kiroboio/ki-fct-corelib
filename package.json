{
  "name": "@kiroboio/fct-core",
  "version": "1.11.6",
  "description": "Kirobo.io FCT Core library",
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha -r ts-node/register \"src/**/*.test.ts\"",
    "test:fct": "TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha -r ts-node/register src/batchMultiSigCall/**/*.test.ts",
    "test:utils": "mocha -r ts-node/register src/utils/*.test.ts",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "fct": "ts-node scripts/fct.script.ts",
    "fct:struct": "ts-node scripts/fct_struct.script.ts",
    "fct:get": "ts-node scripts/fct_get.script.ts",
    "fct:approvals": "ts-node scripts/fct_approvals.script.ts",
    "fct:estimate": "ts-node scripts/estimate.script.ts",
    "fct:getDomainData": "ts-node scripts/getDomainData.ts",
    "lint": "eslint  --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiroboio/ki-fct-corelib.git"
  },
  "keywords": [],
  "author": "BK",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/kiroboio/ki-fct-corelib/issues"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint",
          "releaseRules": [
            {
              "tag": "Docs",
              "message": "*README*",
              "release": "patch"
            },
            {
              "tag": "New",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ]
    ]
  },
  "homepage": "https://github.com/kiroboio/ki-fct-corelib#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4.3.4",
    "@types/graphlib": "^2.1.8",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.7",
    "conventional-changelog-eslint": "^3.0.9",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.18",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethereum-waffle": "^4.0.10",
    "fast-json-patch": "^3.1.1",
    "hardhat": "^2.13.0",
    "hardhat-dependency-compiler": "^1.1.3",
    "hardhat-ethers": "^1.0.1",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "rollup": "^3.21.0",
    "rollup-plugin-dts": "^5.3.0",
    "semantic-release": "^20.1.0",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.4",
    "web3.storage": "^4.5.4"
  },
  "dependencies": {
    "@kiroboio/fct-plugins": "^1.1.153",
    "@metamask/eth-sig-util": "^5.0.2",
    "bignumber.js": "^9.1.0",
    "ethers": "^5.7.2",
    "graphlib": "^2.1.8",
    "lodash": "^4.17.21"
  }
}
